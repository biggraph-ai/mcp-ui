{
  "name": "librechat-node-server",
  "version": "1.0.0",
  "type": "module",
  "description": "MCP UI-ready Node.js server tailored for LibreChat clients (stdio or SSE).",
  "main": "dist/http.js",
  "scripts": {
    "build": "tsc",
    "start:sse": "node dist/http.js",
    "start:stdio": "node dist/stdio.js",
    "dev:sse": "tsx src/http.ts",
    "dev:stdio": "tsx src/stdio.ts"
  },
  "keywords": [
    "mcp",
    "librechat",
    "@mcp-ui/server"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@mcp-ui/server": "^5.2.0",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^20.0.0",
    "ts-node": "^10.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.2.0"
  }
}
